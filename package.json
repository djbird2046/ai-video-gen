{
  "name": "aivideogen",
  "version": "0.1.0",
  "description": "Unified TypeScript SDK for AI video generation (Veo, Sora2, 即梦, 可灵).",
  "author": "djbird2046",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/djbird2046/ai-video-gen"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\" \"examples/**/*.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "video",
    "sdk",
    "ai",
    "veo",
    "sora",
    "keling",
    "jimeng",
    "typescript"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
